{
    "name": "speelpenning/laravel-authentication",
    "description": "Fully configurable authentication for Laravel 5.2.",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerben Speelpenning",
            "email": "gerben@speelpenning.nl"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "speelpenning/contracts": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Speelpenning\\Authentication\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}